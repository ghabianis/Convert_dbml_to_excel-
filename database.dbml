
Table "User" {
  "createdAt" DateTime [not null, default: `now()`]
  "id" String [pk]
  "updatedAt" DateTime
  "deletedAt" DateTime
  "firstName" String
  "lastName" String
  "isBanned" Boolean
  "email" String [unique, not null]
  "password" String [not null]
  "role" String [not null]
  "numberPhone" int
}

Table "Salarier" {
  "id" String [pk]
  "teamId" String
  "userId" String
}

Table "RH" {
  "id" String [pk]
  "companyId" String
  "userId" String
}

Table "Group" {
  "createdAt" DateTime [not null, default: `now()`]
  "id" String [pk]
  "updatedAt" DateTime
  "deletedAt" DateTime
  "name" String [unique, not null]
  "companyId" String
}

Table "Team" {
  "createdAt" DateTime [not null, default: `now()`]
  "id" String [pk]
  "updatedAt" DateTime
  "deletedAt" DateTime
  "name" String [unique, not null]
  "isDefault" Boolean
  "groupId" String
}

Table "Skill" {
  "createdAt" DateTime [not null, default: `now()`]
  "id" String [pk]
  "updatedAt" DateTime
  "deletedAt" DateTime
  "name" String [not null]
  "description" String
  "image" String
  "active" Boolean
  "badgeActive" String
  "badgeDesactive" String
}

Table "Chapter" {
  "createdAt" DateTime [not null, default: `now()`]
