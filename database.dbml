}

Table "Company" {
  "createdAt" DateTime [not null, default: `now()`]
  "id" String [pk]
  "updatedAt" DateTime
  "deletedAt" DateTime
  "name" String [not null]
  "email" String
  "active" String
}

Table "Response" {
  "createdAt" DateTime [not null, default: `now()`]
  "id" String [pk]
  "updatedAt" DateTime
  "deletedAt" DateTime
  "SalarierId" String [not null]
  "chapterId" String [not null]
